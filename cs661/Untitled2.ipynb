    "def sales(price, rate):\n",
    "    sales_tax = price + (price*(rate/100))\n",
    "\n",
    "    total = price + sales_tax\n",
    "    return sales_tax, total\n",
    "\n",
    "price = 100\n",
    "rate = 6.25\n",
    "sales_tax, total = sales(price, rate)\n",
    "print(f\"{sales_tax}\")\n",
    "print(f\"{total}\")"
   ]
